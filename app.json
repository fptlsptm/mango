{
  "expo": {
    "name": "생망고",
    "slug": "mango",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "coms.dmon.mango",
      "supportsTablet": true
    },
    "android": {
      "package": "com.dmon.mango",
      "googleServicesFile": "./google-services.json"
    },
    "notification": {
      "icon": "./assets/images/icon_push.png"  
    }
  }
}
